  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "DEV - IMPORT APP",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}/apps/asset-import",
      "console": "internalConsole",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**/*"],
      "port": 23933,
      "runtimeArgs": ["ts-node", "src/app.ts"],
      // "resolveSourceMapLocations": [
      //     "${workspaceFolder}/**",
      //     "!**/node_modules/**"
      // ],
      "env": {
        "NODE_ENV": "dev",
        "DEBUG": "*"
      }
    }
  ]
}
